{
	"name": "pipeline3",
	"properties": {
		"activities": [
			{
				"name": "Web1",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"linkedServices": [
						{
							"referenceName": "RestService3",
							"type": "LinkedServiceReference"
						}
					],
					"datasets": [
						{
							"referenceName": "RestResource3",
							"type": "DatasetReference"
						}
					],
					"url": "https://jsonplaceholder.typicode.com/users"
				}
			},
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Web1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "id",
									"type": "Int16"
								}
							},
							{
								"source": {
									"path": "$['name']"
								},
								"sink": {
									"name": "name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['username']"
								},
								"sink": {
									"name": "username",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['email']"
								},
								"sink": {
									"name": "email",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['address']['street']"
								},
								"sink": {
									"name": "street",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['address']['suite']"
								},
								"sink": {
									"name": "suite",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['address']['city']"
								},
								"sink": {
									"name": "city",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['address']['zipcode']"
								},
								"sink": {
									"name": "zipcode",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['address']['geo']['lat']"
								},
								"sink": {
									"name": "lat",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['address']['geo']['lng']"
								},
								"sink": {
									"name": "lng",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['phone']"
								},
								"sink": {
									"name": "phone",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['website']"
								},
								"sink": {
									"name": "website",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['company']['name']"
								},
								"sink": {
									"name": "company_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['company']['catchPhrase']"
								},
								"sink": {
									"name": "catchPhrase",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['company']['bs']"
								},
								"sink": {
									"name": "bs",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "RestResource3",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "output3",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}